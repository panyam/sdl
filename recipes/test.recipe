#!/bin/zsh

sdl load ./examples/contacts/contacts.sdl
sdl use ContactsSystem

sdl gen add g1 server.Lookup 100
sdl gen add g2 server.Insert 1

sdl gen start g1
sdl gen start g2

sdl gen start

# sdl gen add load2 server.Insert 1
# sdl gen start load2
sdl metrics add m1 server Lookup --type count
sdl metrics add m2 server Lookup --type latency
sdl metrics add m3 server Insert --type count
sdl metrics add m4 server Insert --type latency
