#!/bin/zsh

sdl load ./examples/contacts/contacts.sdl
sdl use ContactsSystem

sdl gen add g1 server.Lookup 100
sdl gen add g2 server.Insert 1

sdl gen start g1
sdl gen start g2

sdl gen start

# sdl gen add load2 server.Insert 1
